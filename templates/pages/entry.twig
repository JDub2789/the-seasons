{% extends '_layout' %}

{% block main %}
{% if craft.app.request.segments|first == 'events' and craft.app.request.segment(2)|length %}
   {% include 'pages/_event-entry' %}
{% elseif craft.app.request.segments|first == 'events' %}
  {% include 'pages/_event-index' %}
{% elseif craft.app.request.segments|first == 'news' %}
  {% include 'news/index' %}
{% else %}
{% for block in entry.contentBlocks.all() %}
{% include 'blocks/_entry' %}
{% endfor %}
{% endif %}


{% endblock %}