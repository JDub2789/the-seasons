{
  "require": {
    "craftcms/cms": "3.5.19.1",
    "craftcms/redactor": "2.3.3.2",
    "dolphiq/redirect": "1.0.23",
    "marionnewlevant/element-link": "1.0.0",
    "topshelfcraft/wordsmith": "3.0.4",
    "verbb/cp-nav": "2.0.9",
    "verbb/image-resizer": "2.0.4",
    "verbb/super-table": "2.2.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
